{
  "name": "nup.sh",
  "version": "0.4.0",
  "description": "NPM Update Everything",
  "main": "dist/index.js",
  "bin": {
    "nup.sh": "./dist/index.js"
  },
  "scripts": {
    "start": "npm run build && ./dist/index.js",
    "build": "rm -rf ./dist && rollup -c --bundleConfigAsCjs && chmod +x ./dist/index.js"
  },
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/whizzzkid/nup.sh.git"
  },
  "keywords": [
    "NPM",
    "Update",
    "Path",
    "Dependency",
    "of",
    "Dependency",
    "Dependency",
    "Management"
  ],
  "author": "Whizzzkid (me@nishantarora.in)",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/whizzzkid/nup.sh/issues"
  },
  "homepage": "https://github.com/whizzzkid/nup.sh#readme",
  "dependencies": {
    "chalk": "5.2.0",
    "figlet": "1.5.2",
    "ora": "6.1.2",
    "parse-package-name": "1.0.0",
    "semver": "7.3.8"
  },
  "devDependencies": {
    "@rollup/plugin-json": "6.0.0",
    "@rollup/plugin-terser": "0.3.0",
    "@rollup/plugin-typescript": "11.0.0",
    "@types/chalk": "2.2.0",
    "@types/figlet": "^1.5.5",
    "@types/node": "20.10.7",
    "@types/semver": "7.3.13",
    "npm-watch": "0.11.0",
    "rollup": "3.9.1",
    "rollup-plugin-auto-external": "2.0.0",
    "rollup-plugin-node-globals": "1.4.0",
    "tslib": "2.4.1",
    "typescript": "4.9.4"
  }
}
